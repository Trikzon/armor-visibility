{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/armor-visibility",
        "sources": "https://github.com/Trikzon/armor-visibility",
        "issues": "https://github.com/Trikzon/armor-visibility/issues"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "com.diontryban.armor_visibility.client.ArmorVisibilityClientFabric"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "minecraft": "^1.20.3",
        "ash_api": "^20.4.0-alpha"
    }
}
  